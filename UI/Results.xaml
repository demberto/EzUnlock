<Window x:Class="EzUnlock.Results"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Results" Height="400" Width="400" MinWidth="400" MinHeight="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Expander x:Name="successExp" Grid.Row="0" Header="Success" IsExpanded="True">
            <ListBox x:Name="successBox"/>
        </Expander>
        <Expander x:Name="failureExp" Grid.Row="1" Header="Failure" IsExpanded="True">
            <ListBox x:Name="failureBox"/>
        </Expander>
    </Grid>
</Window>
